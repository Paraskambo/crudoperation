<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>View All Contacts</title>
</head>
<body>
    <ul>
        <li><a href="/">Home</a></li>
        <li><a href="/viewallcontacts">View All contacts</a></li>
      </ul>
    <h1 style="text-align: center;">All Contacts</h1>
    

    <% if(contactData.length > 0) { %>

        
           
            <table>
                <tr>
                    <th>Id</th><th>Full Name</th><th>Mobile</th><th>Email</th><th>Message</th><th>Edit</th><th>Delete</th>
                </tr>
                <% contactData.forEach((item) => { %>
                    <tr>
                        <td><%= item._id %></td><td><%= item.Fullname %></td><td><%= item.Mobile %></td><td><%= item.Email %></td><td><%= item.Message %></td><td>Edit</td><td><a href="/delete/<%= item._id %>">Delete</a></td>
                    </tr>
                    <% }) %>
            </table>
        
    <% } else { %>
        <h1><%= 'No Contact Data Found' %></h1>
    <% } %>
</body>
</html>